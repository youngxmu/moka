<!DOCTYPE html>
<html>
  <head>
    <title>军事理论教学分系统</title>
    <%- include ../common/meta.ejs %>
        <link rel='stylesheet' href='css/user/jsll/list.css' >
    
  </head>
  <body>
    <%- include ../common/header.ejs %>
    <div class="submenu">
      <a href="admin/jsll/list">训练计划</a>
      <a href="admin/jsll/list2" class="active">训练实施</a>
      <a href="admin/jsll/list3">辅助资料</a>
    </div>
    <div class="container">
      <div class="sidebar">
        <div class="tree-opr">
          <a href="javascript:void(0);" class="unfold">展开</a>
          |<a href="javascript:void(0);" class="add">增加</a>
          |<a href="javascript:void(0);" class="edit">编辑</a>
          |<a href="javascript:void(0);" class="del">删除</a>
        </div>
        <div style="overflow:auto;min-height:500px;" >
          <ul id="topic_tree" class="ztree"></ul>
          <ul id="series_tree" class="ztree" style="display:none;"></ul>
        </div>
      </div>
      <div class="main" style="padding-top:30px;font-size:14px;color:#333;">
        <div id="content" class="info-content" ></div>
      </div>
    </div>

    <script type="text/template" id="edit_menu_dlg">
    <div class="">
      <p><input id="info_name" placeholder="输入名称" value="${name}" style="line-height:24px;padding: 0 10px;"></p>
      <p><textarea id="info_content" placeholder="输入内容" style="width:600px;height:400px;">${content}</textarea></p>
    </div>
    </script>
    
    <script type="text/javascript" src="js/admin/jsll/jsll.js"></script>
    <script type="text/javascript" src="js/admin/jsll/list2.js"></script>
    
    <script>
      $(document).ready(function() {
          moka.admin.jsll.list.init();
      });
    </script>
  </body>
</html>
